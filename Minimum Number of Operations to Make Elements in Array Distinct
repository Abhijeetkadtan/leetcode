class Solution {
    public int minimumOperations(int[] nums) {
        int[] freq = new int[101]; // ğŸ“Š Frequency tracker
        int i;
        for (i = nums.length - 1; i >= 0; i--) {
            if (freq[nums[i]] == 1) break; // âŒ Duplicate found
            freq[nums[i]]++; // âœ… Mark as seen
        }
        i += 3; // â• Add 3
        return i / 3; // ğŸ”¢ Return number of operations
    }
}
